{
  "name": "@tailor-platform/dev-cli",
  "description": "Tailor Platform CLI for frontend devs",
  "version": "0.1.0-preview.16",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "bin": {
    "tailordev": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --loader \".eta=file\" --minify",
    "release:preview": "pnpm run build && pnpm publish --tag preview",
    "test": "vitest run --reporter verbose",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit --incremental --pretty"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@tailor-platform/dev-config": "workspace:*",
    "memfs": "^4.6.0",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.1.0",
    "chalk": "^5.3.0",
    "docker-compose": "^0.24.3",
    "eta": "^3.2.0",
    "gh-release-fetch": "^4.0.3",
    "ora": "^8.0.1",
    "rc-config-loader": "^4.1.3"
  },
  "prettier": "@tailor-platform/dev-config/prettier"
}
